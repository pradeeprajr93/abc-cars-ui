{
  "name": "abc-cars",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cleanup:reports": "rimraf .nyc_output && rimraf coverage",
    "e2e": "npm run cleanup:reports && ng run abc-cars:e2e",
    "e2e:headless": "npm run cleanup:reports && ng run abc-cars:e2e-headless",
    "e2e:coverage": "npm run cleanup:reports && npx nyc report --reporter=lcov --reporter=text-summary"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.0.0",
    "@angular-devkit/build-angular": "^14.0.5",
    "@angular/cli": "~14.0.5",
    "@angular/compiler-cli": "^14.0.0",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/schematic": "^2.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@types/jasmine": "~4.0.0",
    "cypress": "latest",
    "istanbul-lib-coverage": "^3.2.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "mkdirp": "^1.0.4",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.7.2",
    "webpack": "^5.73.0"
  }
}
